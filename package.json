{
  "name": "wildberries-frontend-ssr",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=12.16.1"
  },
  "browserslist": [
    "last 3 versions",
    "not dead"
  ],
  "dependencies": {
    "@babel/node": "7.8.7",
    "@babel/runtime": "7.9.6",
    "body-parser": "1.19.0",
    "classnames": "2.2.5",
    "cookie-universal": "2.1.4",
    "core-js": "3.6.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "http-proxy-middleware": "1.0.4",
    "i18next": "19.4.4",
    "i18next-express-middleware": "2.0.0",
    "i18next-fs-backend": "1.0.3",
    "i18next-http-backend": "1.0.12",
    "pretty-error": "2.1.1",
    "promise-sequential": "1.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-i18next": "11.5.0",
    "react-redux": "7.2.0",
    "react-router5": "8.0.1",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "redux-optimistic-ui": "3.1.0",
    "redux-thunk": "2.2.0",
    "regenerator-runtime": "0.13.5",
    "reselect": "4.0.0",
    "router5": "8.0.1",
    "router5-helpers": "8.0.0",
    "router5-plugin-browser": "8.0.1",
    "router5-plugin-logger": "8.0.1",
    "router5-transition-path": "8.0.1",
    "ssr-scripts": "git+https://github.com/mihanizm56/ssr-config.git"
  },
  "devDependencies": {
    "@types/classnames": "2.2.10",
    "@types/express": "4.17.6",
    "@types/history": "4.7.5",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/redux-logger": "3.0.7",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-node": "0.3.3",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "husky": "4.2.5",
    "lint-staged": "10.2.6",
    "prettier": "2.0.5",
    "react-deep-force-update": "2.1.3",
    "stylelint": "13.5.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0",
    "stylelint-scss": "3.17.2"
  },
  "scripts": {
    "start": "ssr-scripts start",
    "build": "ssr-scripts build",
    "build-analyse": "npm run build -- --release --analyse",
    "build-start": "npm run build -- --release && node ./build/server.js",
    "update": "npx npm-check -u",
    "lint-scripts": "eslint . --ext .ts,.js,.tsx",
    "lint-scripts-fix": "eslint . --ext .ts,.js,.tsx --fix",
    "lint-styles": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint-styles-fix": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\" --fix",
    "lint": "npm run lint-scripts && npm run lint-styles"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "node_modules/.bin/eslint --ext .ts,.js,.tsx --fix",
      "node_modules/.bin/eslint --ext .ts,.js,.tsx --max-warnings=0"
    ],
    "*.scss": [
      "node_modules/.bin/stylelint --fix"
    ]
  }
}
